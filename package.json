{
  "peerDependencies": {
    "@gorhom/bottom-sheet": "^4.6.4",
    "react": ">=16.8.0",
    "react-native": ">=0.60.0",
    "react-native-gesture-handler": ">=2.0.0",
    "react-native-reanimated": ">=2.0.0",
    "react-native-safe-area-context": ">=4.0.0"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@expo/vector-icons": "^14.0.3",
    "expo": "^51.0.32",
    "expo-constants": "~16.0.2",
    "react-native-web": "^0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@gorhom/bottom-sheet": "^4.6.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-community/slider": "4.5.2",
    "@storybook/addon-ondevice-actions": "^8.3.5",
    "@storybook/addon-ondevice-controls": "^8.3.5",
    "@storybook/react-native": "^8.3.5",
    "@storybook/react-native-web-vite": "^0.0.0",
    "@types/react": "~18.2.79",
    "@types/react-native": "^0.72.8",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "react-native-builder-bob": "^0.35.2",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-svg": "15.2.0",
    "typescript": "~5.3.3",
    "vite": "^5.4.11"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "storybook-generate": "sb-rn-get-stories -js",
    "storybook": "cross-env STORYBOOK_ENABLED='true' expo start",
    "storybook:ios": "cross-env STORYBOOK_ENABLED='true' expo start --ios",
    "storybook:android": "cross-env STORYBOOK_ENABLED='true' expo start --android",
    "prepare": "bob build",
    "typescript": "tsc --noEmit"
  },
  "version": "1.0.8",
  "name": "jas-comp-react",
  "packageManager": "yarn@4.5.0",
  "exports": {
    ".": {
      "types": "./lib/typescript/index.d.ts",
      "import": "./lib/module/index.js",
      "require": "./lib/commonjs/index.js"
    }
  },
  "files": [
    "components",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!.storybook",
    "!*.config.js",
    "!*.log"
  ],
  "react-native-builder-bob": {
    "source": "components",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "tsc": "node_modules/.bin/tsc"
        }
      ]
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
